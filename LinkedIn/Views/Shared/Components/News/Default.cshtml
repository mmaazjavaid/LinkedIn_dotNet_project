@model List<LinkedIn.Models.News> 

    @for(var i = 0; i < 3; i++)
    @foreach ( LinkedIn.Models.News n in Model)
                {
                   <div class="news-feed-news-news">
                    <div class="news-feed-news-news-person">
                        <img src="@(n.PersonImage)" alt="" srcset="">
                        <h5> @n.Name </h5>
                    </div>
                    <div class="news-feed-news-news-image">
                        <img src="@(n.NewsImage)" alt="" srcset="">
                    </div>
                    <div class="news-feed-news-news-likes">
                        <i class="fa fa-thumbs-o-up" style="color: #2867B2;" aria-hidden="true"></i>&nbsp; @n.Likes likes
                    </div>
                    <div class="news-feed-news-news-like">
                        <div><i class="fa fa-thumbs-o-up" aria-hidden="true"></i>&nbsp;Like</div>
                        <div><i class="fa fa-comment" aria-hidden="true"></i>&nbsp;Comment</div>
                        <div><i class="fa fa-share-alt" aria-hidden="true"></i>&nbsp;Share</div>
                        <div><i class="fa fa-paper-plane" aria-hidden="true"></i>&nbsp;Send</div>
                    </div>
                </div>
                  
                }


